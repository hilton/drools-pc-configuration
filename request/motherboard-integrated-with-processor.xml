<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- 
	Request: integrated motherboard (which includes a processor), standard processor
	Response: message "Integrated motherboard does not allow a processor"
 -->
<knowledgebase-request>

	<queries>
		<query-type>
			<queryName>processors</queryName>
			<factNames><string>value</string></factNames>
		</query-type>
		<query-type>
			<queryName>messages</queryName>
			<factNames><string>value</string></factNames>
		</query-type>
	</queries>

	<inOutFacts>
		<named-fact>
			<id>selection</id>
			<fact class="com.lunatech.configuration.Selection">
				<motherboardType>integrated</motherboardType>
				<processorType>standard</processorType>
			</fact>
		</named-fact>
	</inOutFacts>
	
</knowledgebase-request>