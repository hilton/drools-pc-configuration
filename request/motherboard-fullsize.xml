<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- 
	Request: full-size motherboard
	Response: only advanced processor
 -->
<knowledgebase-request>

	<queries>
		<query-type>
			<queryName>processors</queryName>
			<factNames><string>value</string></factNames>
		</query-type>
		<query-type>
			<queryName>messages</queryName>
			<factNames><string>value</string></factNames>
		</query-type>
	</queries>

	<inOutFacts>
		<named-fact>
			<id>selection</id>
			<fact class="com.lunatech.configuration.Selection">
				<motherboardType>fullSize</motherboardType>
			</fact>
		</named-fact>
	</inOutFacts>
	
</knowledgebase-request>